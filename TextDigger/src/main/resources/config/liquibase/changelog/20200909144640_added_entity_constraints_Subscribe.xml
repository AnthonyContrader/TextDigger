<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Subscribe.
    -->
    <changeSet id="20200909144640-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableName="subscribe"
                                 constraintName="fk_subscribe_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="utente"/>

        <addForeignKeyConstraint baseColumnNames="interestgroup_id"
                                 baseTableName="subscribe"
                                 constraintName="fk_subscribe_interestgroup_id"
                                 referencedColumnNames="id"
                                 referencedTableName="interest_group"/>

    </changeSet>
</databaseChangeLog>
